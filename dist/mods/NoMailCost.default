classname=com.wurmonline.ulviirala.mods.NoMailCost
classpath=NoMailCost.jar

# Disable postage cost?
#
# [default: true]
disablePostage=true

# Enable Courier enchant on newly created mailboxes?
#
# [default: true]
enableEnchant=true

# Power of the enchant.
# Only values from 1.0 to 101.0 are accepted
#
# [default: 30.0]
enchantPower=30.0
